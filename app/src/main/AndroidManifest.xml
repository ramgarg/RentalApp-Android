                  android:screenOrientation="unspecified"

        />-->
        <activity android:name="com.eazyrento.customer.dashboard.view.activity.CustomerOrderSummaryActivity"
                  android:screenOrientation="unspecified"
                android:launchMode="singleTask"

        />


        <activity android:name="com.eazyrento.merchant.view.activity.MerchantOrderSummaryActivity"
                android:screenOrientation="unspecified"


                />

        <activity android:name="com.eazyrento.customer.dashboard.view.activity.CustomerPaymentActivity"
                  android:screenOrientation="unspecified"
                android:windowSoftInputMode="adjustPan"

        />

        <activity android:name="com.eazyrento.agent.view.activity.AgentPaymentActivity"
                android:screenOrientation="unspecified"
                android:windowSoftInputMode="adjustPan"

                />

        <activity android:name="com.eazyrento.customer.dashboard.view.activity.ProductDetailsActivity"

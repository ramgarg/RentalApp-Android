                android:screenOrientation="unspecified"

                />
        <activity android:name="com.eazyrento.merchant.view.activity.RateAndReviewActivity"
                android:screenOrientation="unspecified"

                />

        <activity android:name="com.eazyrento.customer.dashboard.view.activity.CustomerFeedbackActivity"
                android:screenOrientation="unspecified"

                />

        <activity android:name="com.eazyrento.agent.view.activity.AgentFeedbackActivity"
                android:screenOrientation="unspecified"

                />


        <activity android:name="com.eazyrento.login.view.ForgotPasswordActivity"
                  android:screenOrientation="unspecified"

        />
        <activity android:name="com.eazyrento.login.view.OTPActivity"
                  android:screenOrientation="unspecified"

        />

        <activity android:name="com.eazyrento.customer.myaddress.view.MyAddressListActivity"
                  android:screenOrientation="unspecified"

        />

        <activity android:name="com.eazyrento.customer.myaddress.view.AddNewAddressActivity"
                  android:screenOrientation="unspecified"
